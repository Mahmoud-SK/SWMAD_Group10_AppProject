<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_likedMemeDetail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.LikedMemeDetailActivity">

    <ImageView
        android:id="@+id/img_likedMemeDetail"
        android:layout_width="376dp"
        android:layout_height="554dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.09"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <Button
        android:id="@+id/btn_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back to "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_likedMemeDetail" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="376dp"
        android:layout_height="44dp"
        android:layout_marginBottom="24dp"
        android:hint="Edit your bottom text"
        app:layout_constraintBottom_toBottomOf="@+id/img_likedMemeDetail"
        app:layout_constraintEnd_toEndOf="@+id/img_likedMemeDetail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/img_likedMemeDetail" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="376dp"
        android:layout_height="44dp"
        android:layout_marginTop="24dp"
        android:hint="Edit your bottom text"
        app:layout_constraintStart_toStartOf="@+id/img_likedMemeDetail"
        app:layout_constraintTop_toTopOf="@+id/img_likedMemeDetail" />
</androidx.constraintlayout.widget.ConstraintLayout>